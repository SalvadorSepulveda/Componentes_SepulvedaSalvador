<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>

<fx:root type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="textField">
         <HBox.margin>
            <Insets right="10.0" />
         </HBox.margin></TextField>
      <Button fx:id="btnControl" mnemonicParsing="false" text="Click Me" />
   </children>
</fx:root>
